# Copy this file to .env.local and fill in values for local development
# IMPORTANT: Do NOT commit your real keys. Keep .env.local out of version control.

# Clerk (Vite requires the VITE_ prefix)
VITE_CLERK_PUBLISHABLE_KEY=YOUR_CLERK_PUBLISHABLE_KEY

# Server
PORT=5000

# Database
MONGODB_URI=mongodb+srv://<username>:<password>@cluster0.mongodb.net/your_db_name

# Trading / Market Data APIs
KITE_API_KEY=
KITE_API_SECRET=
KITE_REDIRECT_URI=http://localhost:5000/redirect
TWELVEDATA_API_KEY=
FINNHUB_API_KEY=

# AI Provider (Gemini)
GEMINI_API_KEY=

# Optional
NODE_ENV=development

